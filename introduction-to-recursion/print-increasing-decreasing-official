//print-increasing-decreasing-official


#include<iostream>
using namespace std;

void incre(int n)
{
    if(n==0)
    return;
    
    
    incre(n-1);
    cout<<n<<endl;
    
}

void decre(int n)
{
    if(n==0)
    return;
    
    cout<<n<<endl;
    decre(n-1);
    
}



int main()
{
int n;cin>>n;
decre(n);
incre(n);

}
